sults.innerHTML = '';\n  // Loop Through Pets\n  pets.forEach(pet => {\n    console.log(pet);\n    const div = document.createElement('div');\n    div.classList.add('card', 'card-body', 'mb-3');\n    div.innerHTML = `\n      <div class=\"col-md-12\">\n        <div>\n          <h4>${pet.name.$t} (${pet.age.$t})</h4>\n          <p class=\"text-secondary\">${pet.breeds.breed.$t}</p>\n          <p>${pet.contact.address1.$t} ${pet.contact.city.$t} ${\n            pet.contact.state.$t\n          } ${pet.contact.zip.$t}</p>\n        </div>\n      <div  class=\"image-content\" style=\"width:auto;height:100%;overflow:hidden;\">\n      <div class=\"spinner-border\" role=\"status\">\n      </div>\n    ` + getDog();\n    `</div>`;\n\n    results.appendChild(div);\n  });\n};"},"lineCount":null}},"error":null,"hash":"4b602822de816752c6ce32b31d43c301","cacheData":{"env":{}}}