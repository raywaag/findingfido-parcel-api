art(function(){\n    $(\"#wait\").css(\"display\", \"block\");\n  });\n  $(document).ajaxComplete(function(){\n    $(\"#wait\").css(\"display\", \"none\");\n  });\n\n\n// Show Listings Of Pets\nfunction showShelters(shelters) {\n  const results = document.querySelector('#results');\n  // Clear First\n  results.innerHTML = '';\n  // Loop Through Pets\n  shelters.forEach(shelter => {\n    console.log(shelter);\n    const div = document.createElement('div');\n    div.classList.add('card', 'card-body', 'mb-3');\n    div.innerHTML = `\n      <div class=\"row\">\n        <div class=\"col-sm-6\">\n          <h4>${shelter.name.$t}</h4>\n          <p>\n            ${shelter.city.$t}\n            ${shelter.state.$t} ${shelter.phone.$t}\n           </p>\n        </div>\n        </div>\n        <div  class=\"image-content\" style=\"width:auto;height:100%;overflow:hidden;\">\n        <div class=\"spinner-border\" role=\"status\"></div>\n    ` + getDog();\n    `</div>`;\n\n    results.appendChild(div);\n  });\n}\n"},"lineCount":null}},"error":null,"hash":"2942dde7662ea30184cb92553c23b837","cacheData":{"env":{}}}